@model IEnumerable<Sis_Atualizacoes.Models.PacotesAtualizacoes>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NumVersao)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RegistroAlteracoes)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DtLancamento)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdProjNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NumVersao)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RegistroAlteracoes)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DtLancamento)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdProjNavigation.IdProjeto)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdPacote">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdPacote">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdPacote">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
